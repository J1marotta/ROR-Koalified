<div class="page-wrapper">


<p id="notice"><%= notice %></p>

<div class="field">
  <p>
    <strong>Name:</strong>
    <%= @profile.name %>
  </p>
</div>


<div class="field">
  <p class:"avatar">
    <strong>Avatar</strong>
    <img src='<%=@profile.avatar_url  %>'>
  </p>

  <p>
    <strong>License:</strong>
    <img src='<%=@profile.license_url  %>'>
  </p>
</div>


<div class="field">
  <p>
    <strong>Driving history and Police check</strong>
    "coming Soon"
  </p>
</div>

<div class="field">
  <p>
    <strong>Bio:</strong>
    <%= @profile.bio %>
  </p>
</div>

<div class="field">
  <p>
    <strong>Car:</strong>
    <%= @profile.car %>
  </p>
</div>

<%= link_to 'Edit', edit_profile_path(@profile) %> |
<%= link_to 'Back', profile_path %>
</div>
